{
  "name": "@dsnp/sdk",
  "version": "2.0.0",
  "description": "SDK Library for the DSNP",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "repository": "https://github.com/LibertyDSNP/sdk-ts",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:multimodules && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:types": "tsc -p tsconfig.types.json && mkdir -p ./dist/types/types && rsync -a --filter='+ */' --filter='-! **/*.d.ts' ./src/types/* ./dist/types/types",
    "build:esm": "tsc -p tsconfig.esm.json && echo '{\"type\": \"module\"}'> dist/esm/package.json",
    "build:multimodules": "node scripts/multimodules.js",
    "coverage": "jest test --runInBand --coverage --collectCoverageFrom='src/**/*.ts'",
    "clean": "rm -Rf dist docs core generators ./*.d.ts ./*.js ./*.tgz",
    "doc": "rm -Rf dist/docs && run-s doc:html",
    "doc:html": "typedoc --options ./typedoc.json --plugin none --theme default --out dist/docs",
    "doc:config": "typedoc --showConfig --options ./typedoc.json --plugin none --theme default --out dist/docs",
    "doc:json": "typedoc --options ./typedoc.json --json dist/docs/typedoc.json",
    "doc:markdown": "rm -Rf docs && typedoc --options ./typedoc.json --theme markdown --out docs",
    "format": "eslint \"**/*.{js,ts}\" --fix",
    "lint": "eslint \"**/*.{js,ts}\"",
    "prepublishOnly": "npm run build && npm run doc:markdown",
    "prepare": "npm run typechain",
    "postpublish": "npm run clean",
    "test": "jest --runInBand",
    "typechain": "rm -Rf ./src/types/typechain && typechain --target=ethers-v5 ./node_modules/@dsnp/contracts/abi/**/*.json --out-dir ./src/types/typechain",
    "watch": "run-s build && run-p \"build:cjs -- -w\" \"build:esm -- -w\" \"build:types -- -w\" \"test -- --watch\""
  },
  "files": [
    "dist/**/*",
    "docs/**/*",
    "core/**/*",
    "generators/**/*",
    "CONTRIBUTING.md",
    "*.js",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=14.15.5"
  },
  "devDependencies": {
    "@dsnp/test-generators": "^0.1.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@types/jest": "26.0.24",
    "@types/mkdirp": "^1.0.2",
    "@types/request": "^2.48.7",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "babel-jest": "^27.0.6",
    "dotenv": "^10.0.0 ",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "27.0.6",
    "jest-junit": "12.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "stream": "^0.0.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typechain": "^5.0.0",
    "typedoc": "0.21.5",
    "typedoc-plugin-markdown": "3.10.4",
    "typescript": "4.3.5"
  },
  "jest": {
    "setupFiles": [
      "./jest.config.js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testResultsProcessor": "jest-junit",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "dependencies": {
    "@dsnp/contracts": "1.0.0",
    "@dsnp/parquetjs": "^1.1.1",
    "@dsnp/test-generators": "^0.1.0",
    "@ethersproject/abi": "^5.4.0",
    "ethers": "^5.3.0",
    "js-sha3": "^0.8.0",
    "ws": "^8.0.0"
  }
}
